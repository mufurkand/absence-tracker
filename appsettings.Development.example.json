{
    "ConnectionStrings": {
        "DefaultConnection": "Host=localhost;Database=your_development_database;Username=your_username;Password=your_password"
    },
    "JwtSettings": {
        "SecretKey": "your-development-secret-key-at-least-32-characters-long",
        "Issuer": "YourAppName-Dev",
        "Audience": "YourAppUsers-Dev",
        "ExpirationInMinutes": 120
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    }
}
